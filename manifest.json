{
  "name": "Nylas Calendar Rooms",
  "version": "1.1",
  "description": "Plugin to easily add the Room Video URLs to Nylas calendar events. ",
  "manifest_version": 2,
   "icons": {
    "128": "logo-128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://calendar.google.com/*"],
      "js": ["contentScript.js"]
    }
  ],
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "logo-128.png"
  }
}
